<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">

<hibernate-mapping>
    <class name="com.sam_solutions.ealys.entity.Image" table="images">
        <id name="imageId" column="image_id" type="java.lang.Long">
            <generator class="identity"/>
        </id>
        <property name="imageName" length="50" column="image_name" type="java.lang.String"/>
    </class>
</hibernate-mapping>